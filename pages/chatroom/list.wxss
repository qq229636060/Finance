/* pages/chatroom/list.wxss */
.roombox{border-bottom: solid 10rpx #f4f4f4;padding:40rpx 30rpx; box-sizing: border-box;}
.roombox_t{display: flex; align-items: center;  }
.face{ width: 100rpx; height: 100rpx;}
.face image{ width: 100%; height: 100%; border-radius: 50%;}
.roomtext{margin-left: 20rpx; width: 420rpx;}
.roomtext_t{font-size: 42rpx;}
.roomtext_b{font-size: 22rpx; color: #323232;}
.bottom_dy{ width: 140rpx; height: 60rpx; font-size: 38rpx; line-height: 60rpx; border-radius: 10rpx; color: #fb432a;box-shadow: 0 0 10rpx #ccc; border:1rpx solid #ccc; text-align: center;}
.roombox_b{ width:650rpx; height: 100rpx; color:#fb432a; font-size:26rpx;box-shadow: 0 0 20rpx #ccc; border:1rpx solid #fb432a; margin: 0 auto; border-radius: 10rpx; margin-top: 30rpx; display: flex; box-sizing: border-box; align-items: center; padding: 0 30rpx; box-sizing: border-box;}
.roombox_b i{ width: 28rpx; height: 28rpx; margin-right: 10rpx; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100%;}
.txt{ width: 430rpx;}
.roombox_b text{ width: 135rpx; height: 45rpx; line-height: 45rpx; border-radius: 25rpx;border:1rpx solid #fb432a; color: #fb432a; text-align: center;}