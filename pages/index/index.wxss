/**index.wxss**/
.top_text{display: flex; justify-content:space-between;align-items: center; padding: 20rpx 30rpx 30rpx 30rpx;}
.top_text view:first-child{ font-size: 38rpx; color: #323232; display: flex; align-items: center;}
.top_text view:first-child i{ width: 50rpx; height: 50rpx; display: inline-block;background-size:100% ; margin-right: 20rpx; background-image:url('data:image/jpeg;base64,');}
.top_text view:first-child .zxicon{background-image: url(data:image/png;base64,);}
.top_text view{ font-size: 32rpx; color: #646464;}
.flex3{display: flex; justify-content: center;}
.flex3 .swiper-item{ width: 33.3%; text-align: center;}
.flex3 .swiper-item view{color: #ff0000;font-size: 30rpx;}
.flex3 .swiper-item view:first-child{color: #323232;}
.flex3 .green view { color: #3e8740;}
.flex3 .swiper-item .f40{font-size: 40rpx;}
.h200{ height: 180rpx;}
.h200 wx-swiper-dots.wx-swiper-dots-horizontal{
  margin-bottom: 5rpx;
  float: left;
  text-align: center;
  }
  .h200 .wx-swiper-dot::before{
  content: '';
  flex-grow: 1;
  background: white;
  border-radius: 15rpx;
  border: 0rpx;
  opacity: 0.5;
  }
  .h200 .wx-swiper-dot.wx-swiper-dot{
  width: 12rpx;
  height: 12rpx;
  border-radius: 12rpx;
  background: white;
  opacity: 0.5;
  }
  .h200 .wx-swiper-dot.wx-swiper-dot-active{
  width: 30rpx;
  height: 12rpx;
  border-radius: 12rpx;
  background:white;
  opacity: 1;
  }

  .index_nav{ background: #f4f4f4; padding:0 0 35rpx 0; display: flex;flex-wrap:wrap; justify-content: center;}
  .navbox{ text-align: center; margin:35rpx 8rpx 0 8rpx; width: 128rpx;}
  .navbox image{ width: 55rpx; height: 55rpx;}
  .navtxt{ font-size: 26rpx; color: #323232;}
  .jdpic{ width: 100%; height: 300rpx;}
  .jdpic image{ width: 100%; height: 300rpx;}
  .newsbox{ width: 100%;display: flex; justify-content: space-between; padding:30rpx; box-sizing: border-box; border-bottom: solid 1rpx #dcdcdc;} 
  .newsbox:first-child{padding-top: 0;}
  .newsbox:last-child{border: none;}
  .newsbox .n_l{ width: 465rpx;}
  .newsbox .n_l view{margin-top: 20rpx;}
  .newsbox .n_l view:first-child{ font-size: 36rpx;margin-top:0; height: 95rpx; overflow: hidden;}
  .newsbox .n_l .time{ font-size: 26rpx; color: #656565;}
  .newsbox .n_l .tag i{font-size: 26rpx; background-color: #f4f4f4;border:solid 1rpx #f66254; color:#f66254; border-radius: 20rpx; padding:3rpx 20rpx; margin-right: 20rpx;} 
  .newsbox .n_r{ width: 200rpx;}
  .newsbox .n_r image{ width: 200rpx; height: 150rpx; border-radius: 10rpx;}
  .noticebar{background: #fc432a; color: #fff; padding:20rpx 30rpx; box-sizing: border-box; display: flex; align-items: center;}
  .h45{ height: 45rpx; margin-left: 20rpx; line-height: 45rpx; overflow: hidden;}
  .w1000{ width: 1000rpx;}
  .f23{font-size: 23rpx;}