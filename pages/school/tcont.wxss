/* pages/school/tcont.wxss */
page{
  background-color:#f4f4f4;
}
.tpic{ width: 100%; height: 400rpx;overflow: hidden; position: relative;}
.tpic image{ width: 100%;}
.fixb{ width: 100%; height: 56rpx; background:rgba(0,0,0,0.5); position: absolute;bottom: 0; display: flex; align-items: center; padding:0 50rpx; box-sizing: border-box; justify-content: space-between}
.fixb_l{font-size: 26rpx; color: #fff;}
.fixb_r{ display: flex; color: #fff; font-size: 22rpx;}
.fixb_r .icon1{ width: 24rpx; height: 15rpx; display: inline-block; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100%; }
.fixb_r .icon2{ width: 23rpx; height: 22rpx; display: inline-block; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100%;}
.fixb_r text{margin-right: 10rpx;}
.fixb_r view{margin-left: 20rpx;}
.fixb_r view:first-child{ margin-left: 0;}
.vcbox{ padding: 30rpx; box-sizing: border-box; display: flex; align-items: center; border-bottom: solid 1rpx #f4f4f4; background:#fff;}
.vbox_l i{ width: 76rpx; height: 76rpx; display: flex; color: #979797; font-size: 48rpx; align-items: center; justify-content: center; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100%;}
.vbox_m {margin-left: 30rpx;}
.vbox_m view:first-child{ font-size: 36rpx; color: #333;}
.vbox_m view{font-size: 24rpx;color: #646464;}
.vbox_r {display: flex;font-size: 20rpx; }
.icon1{ width: 24rpx; height: 15rpx; display: inline-block; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100%; }
.icon2{ width: 23rpx; height: 22rpx; display: inline-block; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100%;}
.vbox_r text{margin-right: 10rpx;}
.vbox_r view{margin-left: 20rpx;}
.vbox_r view:first-child{ margin-left: 0;}