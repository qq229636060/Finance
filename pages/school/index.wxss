/* pages/school/index.wxss */
.line26{ height: 26rpx; background-color: #f0f0f0;}
.titles{ height: 100rpx; display:flex;align-items: center; font-size: 32rpx;border-bottom: solid 1rpx #dcdcdc;}
.titles i{ width: 4rpx; height: 48rpx; display: block; background-color: #fc432a; margin-right: 30rpx; }
.schoolbox{display: flex; padding: 30rpx; box-sizing: border-box; border-bottom: solid 1rpx #dcdcdc;}
.schoolbox .s_l{ width: 205rpx; height: 140rpx; margin-right: 30rpx;}
.schoolbox .s_r{ width: 460rpx;}
.schoolbox .s_l image{ width: 100%;  height: 100%; border-radius: 10rpx;}
.s_title{font-size: 28rpx;}
.s_tag{ margin:15rpx 0 20rpx 0;display: flex; justify-content: space-between; align-items: center;}
.s_tag .s_tag_r{ font-size:20rpx; color: #323232; text-align: right; display:flex;}
.s_tag .s_tag_l i{ font-size: 18rpx; border-radius: 15rpx; color: #fc432a; border: solid 1rpx #fed6d1; padding:5rpx 10rpx;box-shadow: 0 0 10rpx #f00; margin-right: 20rpx;}
.s_jd{ font-size: 20rpx; display: flex; align-items: center; justify-content: space-between;}
.s_tag .s_tag_r text{ margin-right: 10rpx;}
.s_tag .s_tag_r view{ margin-left: 30rpx;}
.s_tag .s_tag_r view:first-child{margin-left: 0;}
.w220{ width: 220rpx;}
.ml50{margin-left: 50rpx;}
.icon1{ width: 24rpx; height: 15rpx; display: inline-block; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100%; }
.icon2{ width: 23rpx; height: 22rpx; display: inline-block; background: url('data:image/png;base64,') no-repeat 0 0; background-size: 100%;}